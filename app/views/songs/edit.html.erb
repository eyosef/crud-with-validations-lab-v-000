<% form_for @song do |f| %>
    <div class="field">
        <%= f.label :title, "Title" %>
        <%= f.text_field :title %>

        <%= f.label :artist_name, "Artist Name" %>
        <%= f.text_field :artist_name %>

        <%= f.label :release_year, "Release Year" %>
        <%= f.text_field :release_year %>

        <%= f.label :released, "Released" %>
        <%= f.text_field :released %>

        <%= f.label :genre, "Genre" %>
        <%= f.text_field :genre %>

        <%= f.submit "Edit Song" %>
    </div>

<% end %>