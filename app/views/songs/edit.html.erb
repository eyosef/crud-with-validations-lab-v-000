<% form_for @ do |f| %>
    <label> f.label :title </label><br>
    <%= f.text_field :title %><br>

    <label> f.label :artist_name </label><br>
    <%= f.text_field :artist_name %><br>

    <label> f.label :release_year </label><br>
    <%= f.text_field :release_year %><br>

    <label> f.label :released </label><br>
    <%= f.text_field :released %><br>

    <label> f.label :genre </label><br>
    <%= f.text_field :genre %><br>

    <%= submit_tag "Edit Song" %>

<% end %>